{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react" 
    ],
    "rules": {        
        // "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
        "react/jsx-filename-extension": [0],
        "import/extensions": "off"
    }
}
